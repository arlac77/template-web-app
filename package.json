{
  "keywords": [
    "web"
  ],
  "template": {
    "inheritFrom": [
      "arlac77/template-npm",
      "arlac77/template-npm-pkgbuild",
      "arlac77/template-semantic-release",
      "arlac77/template-css"
    ],
    "usedBy": [
      "arlac77/npm-dep-graph",
      "arlac77/template-svelte-app"
    ],
    "properties": {
      "web-app": {
        "publish": "public/"
      }
    },
    "mergers": [
      {
        "type": "JSONMerger",
        "pattern": "**/*.webmanifest",
        "options": {
          "expand": false,
          "mergeHints": {
            "*": {
              "overwrite": false
            },
            "": {
              "orderBy": [
                "name",
                "short_name",
                "description",
                "lang",
                "start_url",
                "display",
                "orientation",
                "icons"
              ]
            }
          }
        }
      }
    ]
  },
  "packageManager": "npm@11.6.3+sha512.4085a763162e0e3acd19a4e9d23ad3aa0978e501ccf947dd7233c12a689ae0bb0190763c4ef12366990056b34eec438903ffed38fde4fbd722a17c2a7407ee92"
}
